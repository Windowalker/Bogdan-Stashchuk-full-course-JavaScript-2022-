// accessing the value of a property using parentheses
const myCity = {
  city: "New York",
};
myCity["popular"] = true;

console.log(myCity);
// {city: 'New York, popular: true}

const countryPropertyName = "country";
myCity[countryPropertyName] = "USA";
console.log(myCity);
// {city: 'New York', popular: true, country: 'USA'}
