const myCity = {
  // Создал объект {}означает созание объекта
  city: "New York", // свойство  (имя свойства-city, значение свойства-New york)
  cityGreeting: function () {
    console.log("Greetings!!"); // метод (функция, которая является свойством объекта)
  },
};
myCity.cityGreeting(); //myCity хранит свойства city New York, cityGreeting хранит метод Greetings. Вызов через ()
// Таким образом cityGreeting-это метод объекта myCity, а city просто свойство, потому что его значение не является функцией.

//Сокращенный формат записи методов
const myCity2 = {
  city: "New York",
  cityGreeting() {
    // убрал ключевое слово function. Без доветочия. Скобки так же считываются как функция
    console.log("Greetings!!");
  },
};
myCity2.cityGreeting(); // Вызов метода так же через точечную запись.

// Методы vs свойства объекта
// Свойства не содержат функции, а метод содержит функцию как значение.
// Соответсвенно для доступа к свойству через точечную запись требуется вводить myCity.city
// Доступ к методу через точечн.з. добавляю скобки()
